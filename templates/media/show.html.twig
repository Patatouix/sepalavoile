{% extends 'base.html.twig' %}

{% block title %}Media{% endblock %}

{% block body %}
    <h1>Media</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medium.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ medium.nom }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ medium.url }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ medium.description }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ medium.titre }}</td>
            </tr>
            <tr>
                <th>FacebookLink</th>
                <td>{{ medium.facebookLink }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ medium.createdAt ? medium.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ medium.updatedAt ? medium.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ medium.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('media_index') }}">back to list</a>

    <a href="{{ path('media_edit', {'id': medium.id}) }}">edit</a>

    {{ include('media/_delete_form.html.twig') }}
{% endblock %}
